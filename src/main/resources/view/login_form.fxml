<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="630.0" prefWidth="840.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.LoginFormController">
   <children>
      <Label layoutX="345.0" layoutY="36.0" text="LOGIN">
         <font>
            <Font size="54.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="272.0" layoutY="166.0" onAction="#usernameOnAction" prefHeight="46.0" prefWidth="303.0" promptText="Username" />
      <TextField fx:id="txtPassword" layoutX="272.0" layoutY="241.0" onAction="#passwordOnAction" prefHeight="46.0" prefWidth="303.0" promptText="Password" />
      <Button layoutX="410.0" layoutY="331.0" mnemonicParsing="false" onAction="#loginOnAction" text="Login" />
      <Label layoutX="294.0" layoutY="391.0" text="Don't Have an Account..? " />
      <Label layoutX="443.0" layoutY="391.0" onMouseClicked="#signupOnAction" text="Sign up Here" />
      <Label layoutX="279.0" layoutY="307.0" onMouseClicked="#forgotpassOnAction" text="Forgot Password?" />
      <ComboBox fx:id="cmbType" layoutX="349.0" layoutY="119.0" prefWidth="150.0" />
   </children>
</AnchorPane>
